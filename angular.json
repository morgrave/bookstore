{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bookstore": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/bookstore",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/VS/logs/",
                "output": "/campaigns/VS/logs/"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/VS/images/66",
                "output": "/campaigns/VS/images/66"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/UDT/logs/",
                "output": "/campaigns/UDT/logs/"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/UDT/images/74",
                "output": "/campaigns/UDT/images/74"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/DCF/logs/",
                "output": "/campaigns/DCF/logs/"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/DCF/images/6",
                "output": "/campaigns/DCF/images/6"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/ARC/logs/",
                "output": "/campaigns/ARC/logs/"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/ARC/images/35",
                "output": "/campaigns/ARC/images/35"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/NJS/logs/",
                "output": "/campaigns/NJS/logs/"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/BBC/logs/",
                "output": "/campaigns/BBC/logs/"
              },
              {
                "glob": "**/*.*",
                "input": "../bookshelf/src/campaigns/BBC/images/3",
                "output": "/campaigns/BBC/images/3"
              },
              "src/campaigns/SKT/logs/"
            ],
            "styles": [
              "node_modules/@clr/ui/clr-ui.min.css",
              "src/assets/fonts/SDMiSaeng.css",
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "assets": [
                "src/favicon.ico",
                "src/assets"
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bookstore:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bookstore:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "bookstore:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "bookstore:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bookstore:serve:production"
            }
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy",
          "options": {
            "baseHref": "/bookstore/"
          }
        }
      }
    }
  },
  "defaultProject": "bookstore",
  "cli": {
    "analytics": false
  }
}
